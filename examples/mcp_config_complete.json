{
  "mcpServers": {
    "git": {
      "command": "docker",
      "args": ["run", "--rm", "-i", "mcp/git"],
      "timeout": 45,
      "description": "Git repository operations and analysis"
    },
    "time": {
      "command": "docker",
      "args": ["run", "--rm", "-i", "mcp/time"],
      "timeout": 30,
      "description": "System time and scheduling operations"
    },
    "fetch": {
      "command": "docker",
      "args": ["run", "--rm", "-i", "mcp/fetch"],
      "timeout": 60,
      "description": "Web requests and online research"
    },
    "sequentialthinking": {
      "command": "docker",
      "args": ["run", "--rm", "-i", "mcp/sequentialthinking"],
      "timeout": 45,
      "description": "Advanced reasoning and sequential thinking"
    },
    "memory": {
      "command": "docker",
      "args": ["run", "--rm", "-i", "mcp/memory"],
      "timeout": 45,
      "description": "Persistent agent memory and context"
    },
    "filesystem": {
      "command": "docker",
      "args": ["run", "--rm", "-i", "mcp/filesystem"],
      "timeout": 30,
      "description": "File system operations and management"
    }
  },
  "mcpSettings": {
    "enabled": true,
    "docker_enabled": true,
    "docker_network": null,
    "globalTimeout": 120,
    "persistentConnections": true,
    "optimisticInitialization": true
  }
}